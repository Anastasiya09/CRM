<% unless @errors %> 
  var $specializations = $('#specializations');
  var $tr = $(document.createElement('tr')),
      $statusInp = $('.new-specialization');

  $tdName = $(document.createElement('td'));
  $tdName.text('<%= @specialization.name %>');
  $tdName.addClass('editable-name');
  $tdEdit = $(document.createElement('td')).append('<%= image_tag('edit.png', class: 'edit')%>' );
  $tdEdit.addClass('text-center')
  $tdRemove = $(document.createElement('td')).append('<%= get_remove_link(specialization_path(@specialization), @specialization.id, "specialization_id" )%>');
  $tdRemove.addClass('text-center')
  $tr.attr('name', 'specialization');
  $tr.attr('id', 'specialization_' + <%= @specialization.id %> )
  $tr.attr('value', <%= @specialization.id %>);
  $tr.append($tdName);
  $tr.append($tdEdit);
  $tr.append($tdRemove);
  $specializations.append($tr);
  $statusInp.val('');
<% else %>
  var $notifier = $('.notifier');
  error('<%= @errors[0] %>', $notifier);
<% end %>
