<%= form_for :@user, url: admin_registration_index_path(@user), html: { method: :post } do |f| %>
  <%= render 'shared/error_messages' %>
     <div class="first_name">
      <%= f.label :first_name %><br />
      <%= f.text_field :first_name %>
     </div>

     <div class="last_name">
        <%= f.label :last_name %><br />
        <%= f.text_field :last_name %>
      </div>
      <div class="last_name">
        <%= f.label :email %><br />
        <%= f.text_field :email %>
      </div>

      <div class="field">
        <%= f.check_box :admin%>
        <%= f.label :admin %>
      </div>

      <%= f.submit "Create account", class: "btn btn-large btn-primary" %>
  <% end %>
