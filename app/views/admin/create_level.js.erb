<% unless @errors %> 
  var $levels = $('#levels');
  var $tr = $(document.createElement('tr')),
      $statusInp = $('.new-level');

  $tdName = $(document.createElement('td'));
  $tdName.text('<%= @level.name %>');
  $tdName.addClass('editable-name');
  $tdEdit = $(document.createElement('td')).append('<%= image_tag('edit.png', class: 'edit')%>' );
  $tdEdit.addClass('text-center')
  $tdRemove = $(document.createElement('td')).append('<%= get_remove_link(level_path(@level), @level.id, "level_id" )%>');
  $tdRemove.addClass('text-center')
  $tr.attr('name', 'level');
  $tr.attr('id', 'level_' + <%= @level.id %> )
  $tr.attr('value', <%= @level.id %>);
  $tr.append($tdName);
  $tr.append($tdEdit);
  $tr.append($tdRemove);
  $levels.append($tr);
  $statusInp.val('');
<% else %>
  var $notifier = $('.notifier');
  error('<%= @errors[0] %>', $notifier);
<% end %>
